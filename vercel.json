{
  "version": 2,
  "builds": [
    {
      "src": "api/download.py",
      "use": "@vercel/python@3.1.57"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/download",
      "dest": "/api/download.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/download.py": {
      "runtime": "python3.11"
    }
  }
}
